
@{
    ViewBag.Title = "OutgoingPayment";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}

<h2>Outgoing Payment</h2>



<section class="wrapper main-wrapper row">


    <form method="post">

        <div class="col-md-12">
            <section class="box">
                <header class="panel_header">
                    <h2 class="title pull-left">Outgoing Payment</h2>
                    <div class="actions panel_actions pull-right">
                        <a class="box_toggle fa fa-chevron-down"></a>
                        @*<a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>*@
                    </div>
                </header>
                <div class="content-body">
                    <div class="row">
                        <div class="col-sm-12 col-xs-12">
                            <div class="form-group row">
                                <div class="input-group col-md-12">
                                    <div class="col-md-12">
                                        <div class="white-box" style="border: 1px solid #e0e0e0;padding:2px 5px 2px 20px">
                                            <div class="radio-list">
                                                <div class="input-group row">
                                                    <label class="radio-inline p-0 col-sm-3">
                                                        <div class="radio">
                                                            <input type="radio" name="ReceiptType" id="radio1" value="ARC">
                                                            <label for="radio1"> Advance Receipts From Customer </label>
                                                        </div>
                                                    </label>
                                                    <label class="radio-inline p-0 col-sm-3">
                                                        <div class="radio radio-info">
                                                            <input type="radio" name="ReceiptType" checked id="radio2" value="IRC">
                                                            <label for="radio2"> Invoice Receipts From Customer </label>
                                                        </div>
                                                    </label>
                                                    <label class="radio-inline col-sm-3">
                                                        <div class="radio radio-info">
                                                            <input type="radio" name="ReceiptType" id="radio3" value="RCWOI">
                                                            <label for="radio3"> Receipts From Customer W/O Invoice</label>
                                                        </div>
                                                    </label>
                                                    <label class="radio-inline col-sm-2">
                                                        <div class="radio radio-info">
                                                            <input type="radio" name="ReceiptType" id="radio3" value="RAPV">
                                                            <label for="radio3"> Refund Payment From Vendor</label>
                                                        </div>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    @*<div id="InvBtn" class="input-group col-md-2">
                                            <div class="radio-list">
                                                <label class="radio-inline p-0">
                                                    <div class="radio radio-info">
                                                        <input type="radio" name="InvoiceType" checked id="radio1" value="I">
                                                        <label for="radio1"> Item </label>
                                                    </div>
                                                </label>
                                                <label class="radio-inline">
                                                    <div class="radio radio-info">
                                                        <input type="radio" name="InvoiceType" id="radio2" value="M">
                                                        <label for="radio2"> Service </label>
                                                    </div>
                                                </label>
                                            </div>
                                        </div>*@
                                </div>
                            </div>


                            <div class="col-md-3">
                                <label class="form-label">Receipt No</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><a data-toggle="modal" data-target="#CustomerModal" style="color:blue"><i class="fa fa-search"></i></a></span>
                                    <input type="text" class="form-control" id="txtSoDate" readonly>
                                </div>
                            </div>


                            <div class="col-md-3">
                                <label class="form-label">Receipt Date</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input type="text" class="form-control" id="txtSoDate" placeholder="dd/mm/yyyy">
                                </div>
                            </div>





                            <div class="col-md-3">
                                <label class="form-label">Customer</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><a data-toggle="modal" data-target="#VenderModel" style="color:blue"><i class="fa fa-search"></i></a></span>
                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Code" maxlength="5" readonly>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <label class="form-label">Name</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Name">
                                </div>
                            </div>

                            <div class="col-md-3">
                                <label class="form-label">Cost Center</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><a data-toggle="modal" data-target="#CostCenterModel" style="color:blue"><i class="fa fa-search"></i></a></span>
                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Code" readonly>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <label class="form-label">Cost Center</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Name">
                                </div>
                            </div>

                            <div class="col-md-6">
                                <label class="form-label">Remarks</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-comment"></i></span>
                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Remarks">
                                </div>
                            </div>



                            <div class="row pull-right">
                                <button type="button" class="btn btn-danger" id="btnDelete" style="margin-top:27px; width:150px; border-radius:0px;">Voucher</button>

                                <button type="reset" class="btn btn-default" id="btnClear" style="margin-top:27px; width:150px; border-radius:0px;">Pending Invoices</button>

                                <button type="button" class="btn btn-success" id="btnPrint" style="margin-top:27px; width:150px; border-radius:0px;">Payments</button>

                            </div>


                        </div>




                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <ul class="nav nav-tabs primary" style="border-radius:0px; ">
                                <li class="active">
                                    <a href="#tabInvoices" data-toggle="tab" style="border-radius:0px;">
                                        <i class="fa fa-car"></i>Invoices
                                    </a>
                                </li>
                                <li>
                                    <a href="#tabPayment" data-toggle="tab" style="border-radius:0px;">
                                        <i class="fa fa-car"></i>Payment
                                    </a>
                                </li>
                                <li>
                                    <a href="#tabTaxHead" data-toggle="tab" style="border-radius:0px;">
                                        <i class="fa fa-car"></i>Tax Head
                                    </a>
                                </li>
