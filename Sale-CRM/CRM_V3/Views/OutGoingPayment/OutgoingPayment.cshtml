                            <div class="tab-content primary">
                                <div class="tab-pane fade in active" id="tabInvoices">
                                    <div class="form-group row">






                                        <div style="margin-top:30px">
                                            <div class="col-md-3">
                                                <label class="form-label">Invoice No</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon"><a data-toggle="modal" data-target="#InvoicesModel" style="color:blue"><i class="fa fa-search"></i></a></span>
                                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Invoice No">
                                                </div>
                                            </div>

                                            <div class="col-md-3">
                                                <label class="form-label">Invoice Date</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="fa fa-search"></i></span>
                                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Invoice Date">
                                                </div>
                                            </div>

                                            <div class="col-md-3">
                                                <label class="form-label">Inv. Amt.</label>
                                                <div class="input-group">

                                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Invoice Amount">
                                                </div>
                                            </div>

                                            <div class="col-md-3">
                                                <label class="form-label">Outstanding</label>
                                                <div class="input-group">

                                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Outstanding">
                                                </div>
                                            </div>

                                            <div class="col-md-3">
                                                <label class="form-label">Adjustment</label>
                                                <div class="input-group">

                                                    <input type="text" class="form-control" id="txtSoDate" placeholder="Adjustment">
                                                </div>
                                            </div>
                                            <div class="col-md-3">
