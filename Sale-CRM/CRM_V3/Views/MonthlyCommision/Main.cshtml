<section class="wrapper main-wrapper row" style=''>

    <div class="clearfix"></div>
    <!-- MAIN CONTENT AREA STARTS -->
    <form id="icon_validate" action="javascript:;" novalidate="novalidate">
        <div class="col-lg-12">
            <section class="box ">
                <header class="panel_header">
                    <h2 class="title pull-left">Monthly Commission</h2>
                </header>
                <div class="content-body">
                    <div class="row">
                        <div class="col-md-12">

                            <div class="col-md-4">
                                <label class="form-label">Transaction</label>
                                <div class="input-group">

                                    <span class="input-group-addon"><a data-toggle="modal" data-target="#MCModal" style="color:blue"><i class="fa fa-search"></i></a></span>
                                    <input type="text" class="form-control" id="txtTransaction" readonly />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">Transaction Date</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input type="text" class="form-control datepicker" id="txtTransDate" readonly="readonly">
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">Service</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-info"></i></span>
                                    @*<input type="text" class="form-control" id="" maxlength="3" onblur="" name="formfield1" onkeypress="return isNumberKey(event)" />*@
                                    <select class="form-control m-bot15" id="ddlService" onchange="ChangeColor(this)">
                                        <option value="0">Select</option>
                                        <option value="BO">Booking Order</option>
                                        @*<option value="TL">Technician Labor </option>
                                        <option value="JC">JobCard</option>
                                        <option value="EW">Extended Warranty</option>
                                        <option value="UV">Used Vehicle Sales</option>*@
                                    </select>
                                </div>
                                <div class="makespace"></div>
                            </div>
                            <div class="col-md-2">
                                <label class="form-label">Type</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-info"></i></span>
                                    @*<input type="text" class="form-control" id="" maxlength="3" onblur="" name="formfield1" onkeypress="return isNumberKey(event)" />*@
                                    <select class="form-control m-bot15" id="ddlType" onchange="ChangeColor(this)">
                                        <option value="0">Select</option>
                                        <option value="Emp">Employee</option>
                                        <option value="other">Other Comission </option>
                                        
                                    </select>
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-2">
                                <label class="form-label">Commission Month</label>
                                <div class="input-group controls">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <select class="form-control m-bot15" id="ddlMonth" onchange="ChangeColor(this)">
                                        <option value="0">Select</option>
                                        <option value="01">January</option>
                                        <option value="02">Feburary</option>
                                        <option value="03">March</option>
                                        <option value="04">April</option>
                                        <option value="05">May</option>
                                        <option value="06">June</option>
                                        <option value="07">July</option>
                                        <option value="08">August</option>
                                        <option value="09">September</option>
                                        <option value="10">October</option>
                                        <option value="11">November</option>
                                        <option value="12">December</option>
                                    </select>
                                </div>
                                <div class="makespace"></div>
                            </div>

                            
                            <div class="col-md-8">
                                <label class="form-label">Remarks</label>
                                <div class="controls">
                                    <textarea class="form-control autogrow" cols="5"  id="txtRemarks" onblur="ChangeColor(this)" style="overflow: hidden; word-wrap: break-word; resize: none; height: 50px;"></textarea>
                                </div>
                            </div>

                        </div>
                    </div>

                    <hr style="border-top:1px solid blue;" />

                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped" id="MCDetail" style="white-space: nowrap; font-weight: 400;">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Employee</th>
                                            <th>Brand</th>
                                            <th>Product</th>
                                            <th>Version</th>
                                            <th>Color</th>
                                            <th>Reference</th>
                                            <th>Total Qty</th>
                                            <th>Total Amount</th>
                                            <th>Commision Code</th>
                                            <th>Commision %</th>
                                            <th>Commision Amount</th>
                                            <th>Action</th>
                                            <th>Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right">
                                <button type="reset" class="btn btn-default" id="btnClear" style="margin-top:27px;border-radius:0px; width:100px">Clear</button>
                              
                                <button type="button" class="btn btn-info" id="btnSave"  style="margin-top:27px;border-radius:0px; width:100px">Save</button>
                                
                                @*<button type="button" class="btn btn-danger" id="btnDelete" style="margin-top:25px;" disabled>Delete</button>*@
                                <button type="button" class="btn btn-success" id="btnPrint" style="margin-top:27px;border-radius:0px; width:100px">Print</button>
                               
