        if (Frieght == "" || Frieght == null || Frieght == NaN) {
            Frieght = 0;
        }
        var res = 0;


        $("#tblAddOns tbody tr").each(function () {


            var check = $(this).closest('tr').find('#chkAddOns').prop('checked');
            if (check) {
                if($(this).closest('tr').find('#lblPrice').val()=="" ||$(this).closest('tr').find('#lblPrice').val()=="0"||$(this).closest('tr').find('#lblPrice').val()==undefined ||$(this).closest('tr').find('#lblPrice').val()=="null"){
                    FeatureAmount=0;
                }else{
                    FeatureAmount = FeatureAmount + parseFloat($(this).closest('tr').find('#lblPrice').val());
                }


            }
        });

        res = parseFloat(Frieght) + parseFloat(amt) + parseFloat(FeatureAmount);

        return res;

    }
