                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-2">
                            <label class="form-label">OEM Annouce Date</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                <input type="text" class="form-control datepicker" id="txtOEMDate" readonly>
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-2">
                            <label class="form-label">Transpoter Name</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-file"></i></span>
                                @Html.DropDownList("ddlTranspoter", new SelectList((IEnumerable<SelectListItem>)ViewBag.ddlTransPort, "Value", "Text", "0"), new { @class = "form-control m-bot15", style = "border-radius:0px;", @id = "ddlTranspoter" })
                            </div>
                            <div class="makespace"></div>
                        </div>


                        <div class="col-md-2">
                            <label class="form-label">Document No</label>
                            <div class="input-group">
                                <span class="input-group-addon">
                                    <span class="arrow"></span>
                                    <i class="fa fa-file"></i>
                                </span>
                                <input type="text" required class="form-control" id="txtDocNo" maxlength="10">
                            </div>
                            <div class="makespace"></div>
                        </div>

                        <div class="col-md-2">
                            <label class="form-label">Usage</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                <select class="form-control m-bot15" id="ddlUsage" style = "border-radius:0px;">
                                    <option value="0">Select</option>
                                    <option value="New" selected>New</option>
                                    <option value="Used">Used</option>
                                </select>
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
