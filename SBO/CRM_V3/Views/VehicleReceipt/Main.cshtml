    function removeRow(value) {
        rIndex = $(this).closest('tr');
        index = $(this).closest('tr')[0].rowIndex;
        $(this).closest('tr').remove();

        var leadId = $("#ddlRecNo").val();
        var dealerCode = "@Session["DealerCode"].ToString()";
        var lblChassisNo = $(value).closest('tr').find("#lblChassisNo").text().trim();
        var lblEngineNo = $(value).closest('tr').find("#lblEngineNo").text().trim();

        if (leadId != 0) {
            $.ajax({
                dataType: "json",
                async: true,
                type: 'POST',
                url: '@Url.Content("~/VehicleReceipt/Delete_ProdRecDetail")',
                data: { 'EnquiryId': leadId, 'DealerCode': dealerCode, 'ChasisNo': lblChassisNo, 'EngineNo': lblEngineNo },
                success: function (data) {
                    if (data) {
                        if (data.success == true) {
                            $("#section-confirm-Delete").modal("hide")
                            $("#modalMessage").css("background-color", "#64c344");
                            $("#lblMessage").text(data.Message)
                            $("#section-message").modal('show');
                            setTimeout(function () {
                                $('#section-message').modal('hide')
                            }, 4000);
                        } else {
                            $("#section-confirm-Delete").modal("hide")
                            $("#modalMessage").css("background-color", "#e46457");
                            $("#lblMessage").text(data.Message)
                            $("#section-message").modal('show');
                            setTimeout(function () {
                                $('#section-message').modal('hide')
                            }, 4000);

                        }
                    }
                },
                complete: function () {

                },
            });
        } else {
            var tempRow = "fixedRow" + (parseInt($(value).parent().parent().attr("data-tr-count")) - 1);

            $("#" + tempRow).find('.remove').show();
            $(value).parent().parent().remove();

            dataCount--;
            rowCount--;
            SNo--;

        }
    }

