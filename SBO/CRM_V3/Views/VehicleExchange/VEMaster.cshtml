        dto.ChassisNo          = $("#txtNChassisNo").val();
        dto.EngineNo           = $("#txtNEngineNo").val();
        dto.SalesPerson        = $("#ddlEmp").val();
        dto.ExchangeRemarks    = $("#txtRemarks").val();
        dto.NewVehicleType     = NewVehicleType;

        $.ajax({

            dataType: "json",
            async: true,
            type: 'Get',
            url: '@Url.Content("~/VehicleExchange/Insert_Exchange")',
            data: dto,

            success: function (data) {
                if (data) {
                    if (data.Success == true) {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#64c344");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                        setTimeout(function () {
                            $('#section-message').modal('hide')
                        }, 4000);
                    }
                    else {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#e46457");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                        setTimeout(function () {
                            $('#section-message').modal('hide')
                        }, 4000);
                    }
                }
            },


        });

    }

