    //Save
    function SaveEnquiryMaster() {


        var dto = {};

        if ($("#ddlEmp").val() == "") {
            $("#modalMessage").css("background-color", "#e46457");
            $("#lblMessage").text("Please Select Sales Person: ")
            $("#section-message").modal('show');
            setTimeout(function () {
                $('#section-message').modal('hide')
            }, 4000);
            return;

        }
        if ($("#ddlLocation").val() == "") {
            $("#modalMessage").css("background-color", "#e46457");
            $("#lblMessage").text("Please Select Location: ")
            $("#section-message").modal('show');
            setTimeout(function () {
                $('#section-message').modal('hide')
            }, 4000);
            return;
        }
        if ($("#txtBuyingCode").val() == "") {
            $("#modalMessage").css("background-color", "#e46457");
            $("#lblMessage").text("Please Select Buying Code: ")
            $("#section-message").modal('show');
