            $("#lblMessage").text("Please Select Buying Code: ")
            $("#section-message").modal('show');
            setTimeout(function () {
                $('#section-message').modal('hide')
            }, 4000);
            return;

        }
        debugger;

        var NewVehicleType = $('input[name=Booking]:checked').val();


        debugger;

        dto.ExchangeCode       = $("#txtExchangeCode").val();
        dto.ExchangeDate       = $("#txtExchangeDate").val();
        dto.LocationCode       = $("#ddlLocation").val();
        dto.BuyingMode         = $("#ddlBuyingMode").val();
