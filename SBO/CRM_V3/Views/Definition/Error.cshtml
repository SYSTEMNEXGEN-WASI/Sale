
@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}

<h2>Error</h2>


<div class="row">
    <div class="col-md-12">
        <div class="white-box">
            <div class="row">
                <div class="col-sm-12 col-xs-12">
                    <h5>
                        @if (TempData["TestAccessError"] != null)
