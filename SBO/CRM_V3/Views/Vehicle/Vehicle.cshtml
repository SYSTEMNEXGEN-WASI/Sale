
@{
    ViewBag.Title = "Vehicle";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}

<style>
    .makespace {
        margin-bottom: 20px;
    }

    .icheckbox_minimal-blue {
        top: 10px;
    }
</style>
<section class="wrapper main-wrapper row">
    <form method="post">
        <div class="col-xs-12">
            <section class="box">
                <header class="panel_header">
                    <h2 class="title pull-left">Vehicle Information</h2>
                    <div class="actions panel_actions pull-right">
                        <a class="box_toggle fa fa-chevron-down"></a>
                    </div>
                </header>
                <div class="content-body">
                    <div class="row">
                        <div class="col-md-4">
                            <label class="form-label">Vehicle Id</label>
                            <div class="input-group">
                                <span class="input-group-addon"><a id="IssueClick" onclick="ShowModal('Vehicle')" style="color:blue"><i class="fa fa-search"></i></a></span>
                                <input type="text" class="form-control" id="ddlVehicle" readonly>
                                @*@Html.DropDownList("ddlVehicle", new SelectList((IEnumerable<SelectListItem>)ViewBag.Vehicle, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlVehicle" })*@
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                            <label class="form-label">Made/Brand</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-tags"></i></span>
                                @Html.DropDownList("ddlmade", new SelectList((IEnumerable<SelectListItem>)ViewBag.Made, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlmade" })
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                            <label class="form-label">Product Title</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-tasks"></i></span>
                                <input type="text" class="form-control" id="txtProdTitle" maxlength="50">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-2">
                            <label class="form-label">Product Code</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-barcode"></i></span>
                                <input type="text" class="form-control" id="txtProdCode" maxlength="10">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-2">
                            <label class="form-label">Model/Version Code</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-barcode"></i></span>
                                <input type="text" class="form-control" maxlength="3" id="txtVersionCode">
                            </div>
                            <div class="makespace"></div>
                        </div>
                        <div class="col-md-4">
                            <label class="form-label">Vehicle Description</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-file-text-o"></i></span>
                                <input type="text" class="form-control" id="txtShortDesc" maxlength="30">
                            </div>
                            <div class="makespace"></div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">

                            <ul class="nav nav-tabs primary" style="border-radius:0px; border-bottom: 0px;">
                                <li class="active">
                                    <a href="#tabVehDetails" data-toggle="tab" style="border-radius:0px;">
                                        <i class="fa fa-car"></i> Vehicle Details
                                    </a>
                                </li>
                                <li>
                                    <a href="#tabVehPrice" data-toggle="tab" style="border-radius:0px;">
                                        <i class="fa fa-car"></i> Vehicle Price
                                    </a>
                                </li>

                                <li>
                                    <a href="#tabAccCodeSetup" data-toggle="tab">
                                        <i class="fa fa-event"></i> Account Code Setup
                                    </a>
                                </li>
                                @* <li>
                                        <a href="#settings-7" data-toggle="tab">
                                            <i class="fa fa-car"></i> Vehicle Location
                                        </a>
                                    </li>*@
                            </ul>

                            <div class="tab-content">
                                <div class="tab-pane fade in active" id="tabVehDetails">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label class="form-label">Long Description</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-file-o"></i></span>
                                                <input type="text" class="form-control" id="txtLongDesc" maxlength="100">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Vehicle Type</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-cab"></i></span>
                                                @Html.DropDownList("ddlVehicleType", new SelectList((IEnumerable<SelectListItem>)ViewBag.VehicleType, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlVehicleType" })
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Transmission Type</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-flag-o"></i></span>
                                                <select class="form-control m-bot15" id="ddlTransType" name="CusTitle">
                                                    <option value="0">Select</option>
                                                    <option value="Auto">Auto</option>
                                                    <option value="Manual">Manual</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Segment</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-info"></i></span>
                                                <select class="form-control m-bot15" id="ddlSegment">
                                                    <option value="0">Select</option>
                                                    <option value="2 WHEEL">2 WHEEL</option>
                                                    <option value="4 WHEEL">4 WHEEL</option>
                                                    <option value="6 WHEEL">6 WHEEL</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Vehicle Category</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-cab"></i></span>
                                                <input type="text" class="form-control" value="LCV" id="txtVehCategory" readonly>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Vehicle Status</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-bus"></i></span>
                                                <select class="form-control m-bot15" id="ddlVehStatus" name="CusTitle">
                                                    <option value="0">Select</option>
                                                    <option value="New">New</option>
                                                    <option value="Used">Used</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4 hidden">
                                            <label class="form-label">Color</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-pie-chart"></i></span>
                                                @Html.DropDownList("ddlVehColor", new SelectList((IEnumerable<SelectListItem>)ViewBag.Color, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlVehColor" })
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Seater</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-wheelchair"></i></span>
                                                <input type="text" class="form-control" id="txtSeater" maxlength="1">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">No. of Compartment</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-th"></i></span>
                                                <input type="text" class="form-control" id="txtCompartment" maxlength="1">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Fuel Type</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-tint"></i></span>
                                                <select class="form-control m-bot15" id="ddlFuelType" name="CusTitle">
                                                    <option value="Petrol">Petrol</option>
                                                    <option value="Diesel">Diesel</option>
                                                    <option value="LPG">LPG</option>
                                                    <option value="CNG">CNG</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Weight</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-cubes"></i></span>
                                                <input type="text" class="form-control" id="txtWeight" maxlength="6">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Engnine (CC)</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-cogs"></i></span>
                                                <input type="text" class="form-control" id="txtEngCapacity" maxlength="5">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Vehicle Prefix</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-puzzle-piece"></i></span>
                                                <input type="text" class="form-control" id="txtVehPrefix" maxlength="20">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>

                                        <div class="col-md-2">
                                            <label class="form-label">Schedule(Days)</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-calendar-o"></i></span>
                                                <input type="text" class="form-control" id="txtSchPlan" maxlength="8">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>



                                        <div class="col-md-4">
                                            <label class="form-label">MSPR</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtMSPR" maxlength="10">
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                     
                                        
                                      
                                        <div class="row">
                                            <div class="col-md-4">
                                                <input type="checkbox" id="chkFSC" value="Y" /> Free Service Coupons
                                            </div>
                                            <div class="col-md-4">
                                                <input type="checkbox" value="Y" id="chkVehActive" /> Active
                                            </div>
                                            <div class="col-md-4">
                                                <input type="checkbox" value="Y" id="chkVehDummy" /> Dummy Vehicle
                                            </div>
                                            <div style="margin-bottom: 40px;"></div>
                                        </div>
                                        
                                            <div class="col-md-3">
                                                <label class="form-label">Pre Delivery Service</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="fa fa-calendar-o"></i></span>
                                                    <input type="text" class="form-control" id="txtPDI" disabled maxlength="10">
                                                </div>
                                                <div class="makespace"></div>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">First Free Service</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="fa fa-calendar-o"></i></span>
                                                    <input type="text" class="form-control" id="txtFFS" disabled maxlength="10">
                                                </div>
                                                <div class="makespace"></div>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Second Free Service</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="fa fa-calendar-o"></i></span>
                                                    <input type="text" class="form-control" id="txtSFS" disabled maxlength="10">
                                                </div>
                                                <div class="makespace"></div>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="form-label">Third Free Service</label>
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="fa fa-calendar-o"></i></span>
                                                    <input type="text" class="form-control" id="txtTFS" disabled maxlength="10">
                                                </div>
                                                <div class="makespace"></div>
                                            </div>

                                            <div class="col-md-2">
                                                <label class="form-label">Photo</label>
                                                <div class="input-group">
                                                    <input type="file" id="myFile">
                                                </div>
                                                <div class="makespace"></div>
                                            </div>

                                        <div class="col-md-4" style="margin-left: 75px;">
                                            <label class="form-label">Remarks</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-comment"></i></span>
                                                <textarea name="comment" form="usrform" maxlength="100" class="form-control" id="txtRemarks">Enter text here...</textarea>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        
                                    </div>



                                    </div>
                                <div class="tab-pane fade" id="tabVehPrice">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label class="form-label">Color</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-pie-chart"></i></span>
                                                @Html.DropDownList("ddlColor", new SelectList((IEnumerable<SelectListItem>)ViewBag.Color, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlColor" })
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Basic Price</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtSalePrice" maxlength="9" onkeypress="return isNumberKey(event)" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Cost Price</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtCostPrice" maxlength="9" onkeypress="return isNumberKey(event)" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">With Holding Tax (%)</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtWHTax" maxlength="5" onkeypress="return isNumberKey(event)" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Furthur Tax (%)</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtFurTax" maxlength="5" onkeypress="return isNumberKey(event)" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Extra Tax (%)</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtExTax" maxlength="5" onkeypress="return isNumberKey(event)" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Registered Type</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <select class="form-control m-bot15" id="ddlRegType" name="CusTitle">
                                                    <option value="Filer">Filer</option>
                                                    <option value="Non-Filer">Non-Filer</option>
                                                    <option value="Exempted">Exempted</option>
                                                </select>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Register Tax (%)</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-money"></i></span>
                                                <input type="text" class="form-control" id="txtRegTax" maxlength="5" onkeypress="return isNumberKey(event)" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <label class="form-label">Active</label>
                                            <div class="input-group">
                                                <input type="checkbox" id="chkActive" value="Y" class="icheckbox_minimal-white" />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="input-group" style="width:100%;margin-top: 28px;">
                                                <button class="btn btn-success btn-sm" type="button" style="width: 100%;padding: 8px;" onclick="AddVehicleDetail(this)">
                                                    <i class="fa fa-plus fa-2x"></i>
                                                </button>
                                            </div>
                                            <div class="makespace"></div>
                                        </div>

                                    </div>
                                    <div class="row" style="margin-top:1rem;">
                                        <div class="col-md-12">
                                            <div class="table-responsive">
                                                <table class="table table-bordered table-striped" id="VehicleDetail" style="white-space: nowrap; FONT-SIZE: 13px;">
                                                    <thead>
                                                        <tr>
                                                            <th>SN #</th>
                                                            <th>Color</th>
                                                            <th>Basic Price</th>
                                                            <th>Cost Price</th>
                                                            <th>WHTax</th>
                                                            <th>Fur Tax</th>
                                                            <th>Extra Tax</th>
                                                            <th>Reg Type</th>
                                                            <th>Reg Tax</th>
                                                            <th>Active</th>
                                                            @*<th>Status</th>

                                                                <th>Sale Price</th>*@
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="tab-pane fade" id="tabAccCodeSetup">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label class="form-label">Vehicle Stock PSMC.</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><a data-toggle="modal" data-target="#AccountModal" style="color:blue"><i class="fa fa-search" onclick=""></i></a></div>
                                                <input type="text" required class="form-control" style="width:40%" id="txtAccCode" readonly />
                                                <input type="text" required class="form-control" style="width:60%" id="txtAccDesc" readonly />

                                            </div>
                                            <div class="makespace"></div>
                                        </div>

                                        <div class="col-md-4">
                                            <label class="form-label">Cost of Sale PSMC</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><a data-toggle="modal" data-target="#AccountModalCostofSale" style="color:blue"><i class="fa fa-search" onclick=""></i></a></div>
                                                <input type="text" required class="form-control" style="width:40%" id="txtCoseofSaleCode" readonly />
                                                <input type="text" required class="form-control" style="width:60%" id="txtCostofSaleDesc" readonly />
                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Revenue</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><a data-toggle="modal" data-target="#AccountRevenueModal" style="color:blue"><i class="fa fa-search" onclick=""></i></a></div>
                                                <input type="text" required class="form-control" style="width:40%" id="txtRevenueCode" readonly />
                                                <input type="text" required class="form-control" style="width:60%" id="txtRevenueDesc" readonly />

                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Own Money</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><a data-toggle="modal" data-target="#AccountOwnModal" style="color:blue"><i class="fa fa-search" onclick=""></i></a></div>
                                                <input type="text" required class="form-control" style="width:40%" id="txtOwnMoneyC" readonly />
                                                <input type="text" required class="form-control" style="width:60%" id="txtOwnMoneyD" readonly />

                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                        <div class="col-md-4">
                                            <label class="form-label">Down Money</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><a data-toggle="modal" data-target="#AccountDownModal" style="color:blue"><i class="fa fa-search" onclick=""></i></a></div>
                                                <input type="text" required class="form-control" style="width:40%" id="txtDownMoneyC" readonly />
                                                <input type="text" required class="form-control" style="width:60%" id="txtDownMoneyD" readonly />

                                            </div>
                                            <div class="makespace"></div>
                                        </div>
                                    </div>

                                </div>

                            </div>

                        </div>
                    </div>

                    <div class="row">
                        <div class="pull-right">

                            <div class="pull-right">
                                @if (DXBMS.SecurityBll.UserRights("2503", "002"))
                                {
                                    <button type="button" class="btn btn-primary" id="btnSave" style="margin-top:25px;width:100px; border-radius:0px;">Save</button>
                                }
                                else
                                {
                                    <button type="button" class="btn btn-primary" id="btnSave" style="margin-top:25px;width:100px; border-radius:0px;">Save</button>
                                }
                                @if (DXBMS.SecurityBll.UserRights("2503", "004"))
                                {
                                    <button type="button" class="btn btn-danger" id="btnDelete" style="margin-top:25px;width:100px; border-radius:0px;" >Delete</button>
                                }
                                else
                                {
                                    <button type="button" class="btn btn-danger" id="btnDelete" style="margin-top:25px;width:100px; border-radius:0px;" disabled>Delete</button>
                                }

                               
                               
                                <button type="reset" class="btn btn-default" id="btnCancel" style="margin-top:25px;width:100px;border-radius:0px;">Clear</button>
                              </div>

                        </div>
                    </div>
                </div>


            </section>
        </div>

    </form>

    <!-- Popup box modal start -->
    <div class="modal" id="section-confirm" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Confirm</h4>
                </div>
                <div class="modal-body">

                    <h5>Are you sure want to save ?</h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">No</button>
                    <button class="btn btn-primary" type="button" onclick="SaveEnquiry()">Yes</button>
                </div>
            </div>
        </div>
    </div>
