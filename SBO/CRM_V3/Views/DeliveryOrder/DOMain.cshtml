            $("#ddlReceiver").css("border-color", "red");
            count++;
        }
        if (segment == "0" || segment == null) {
            $("#ddlSegment").css("border-color", "red");
            count++;
        }
        if (saleType == "0") {
            $("#ddlSalesType").css("border-color", "red");
            count++;
        }
        if (usage == "0" || usage == null) {
            $("#ddlUsage").css("border-color", "red");
            count++;
        }
        if (assignTo == "" || assignTo == null) {
            $("#ddlAssignTo").css("border-color", "red");
            count++;
        }


        var rowCount = $('#productDetail >tbody  >tr').length;
        if (rowCount == 0) {
            $("#productDetail").css("border-color", "red");
        }

        if (count > 0 || rowCount == 0) {

            $("#lblMessage").html("Please fill the Required feilds");
            $("#section-message").modal("show");
            return false;
        }
        else {
            return true;
        }

    }



