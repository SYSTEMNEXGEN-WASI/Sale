
@{
    ViewBag.Title = "Main";
    Layout = "~/Views/Shared/_LayoutV2.cshtml";
}


<style>
    .makespace {
        margin-bottom: 20px;
    }
</style>


<section class="wrapper main-wrapper row" style=''>

    <div class='col-xs-12'>

    </div>
    <div class="clearfix"></div>
    <!-- MAIN CONTENT AREA STARTS -->
    <form id="icon_validate" action="javascript:;" novalidate="novalidate">
        <div class="col-lg-12">
            <section class="box ">
                <header class="panel_header">
                    <h2 class="title pull-left">Vehicle Location Transfer</h2>
                    <div class="actions panel_actions pull-right">
                        <a class="box_toggle fa fa-chevron-down"></a>
                    </div>
                </header>
                <div class="content-body">
                    <div class="row">
                        <div class="col-xs-12">

                            <div class="col-md-4">
                                <label class="form-label">Transfer No</label>
                                <div class="input-group">

                                    <span class="input-group-addon"><a id="IssueClick" onclick="ShowModal('VehTransLoc')" style="color:blue"><i class="fa fa-search"></i></a></span>
                                    <input type="text" class="form-control" id="txtTransNo" readonly />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">Transfer Date</label>
                                <div class="input-group controls">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input type="text" class="form-control" id="txtTransDate" readonly format />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">Current</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-info"></i></span>
                                    @Html.DropDownList("ddlCurrentLoc", new SelectList((IEnumerable<SelectListItem>)ViewBag.VehLoc, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlCurrentLoc", @onchange = "ChangeColor(this)" })
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">To</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-file"></i></span>
                                    @Html.DropDownList("ddlNewLoc", new SelectList((IEnumerable<SelectListItem>)ViewBag.VehLoc, "Value", "Text", "0"), new { @class = "form-control m-bot15", @id = "ddlNewLoc", @onchange = "ChangeColor(this)" })
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-6">
                                <label class="form-label">Remarks </label>
                                <div class="controls">
                                    <textarea class="form-control autogrow" cols="5" id="txtRemarks" maxlength="200" style="overflow: hidden; word-wrap: break-word; resize: none; height: 50px;"></textarea>
                                </div>
                                <div class="makespace"></div>
                            </div>

                        </div>
                    </div>
                    
                        <hr style="border-top:1px solid blue;" />

                        <h2 class="title pull-left" style="font-size: 19px; font-weight: 400;text-transform: uppercase;display: inline-block;">Vehicle Detail</h2>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="col-md-4">
                                <label class="form-label">VIN / Chassis</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><a id="IssueClick" onclick="ShowModal('Chasis')" style="color:blue"><i class="fa fa-search"></i></a></span>
                                    <input type="text" class="form-control" id="txtChassisNo" readonly  onblur="ChangeColor(this)" />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">Engine</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                    <input type="text" class="form-control" id="txtEngineNo" readonly />

                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-4">
                                <label class="form-label">Brand</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                    <input type="text" class="form-control" id="txtBrandCode" style="width:20%;padding: 3px 3px;" readonly />
                                    <input type="text" class="form-control" id="txtBrandDesc" style="width:80%" readonly />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-6">
                                <label class="form-label">Product</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                    <input type="text" class="form-control" id="txtProduct" style="width:30%;padding: 3px 3px;" readonly />
                                    <input type="text" class="form-control" id="txtProductDesc" style="width:70%" readonly />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-2">
                                <label class="form-label">Model</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-pie-chart"></i></span>
                                    <input type="text" class="form-control" id="txtVersionCode" readonly />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-2">
                                <label class="form-label">Reg No.</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-file-text"></i></span>
                                    <input type="text" class="form-control" id="txtRegNo" readonly />
                                </div>
                                <div class="makespace"></div>
                            </div>

                            <div class="col-md-2">
                                <div class="input-group" style="width:100%;float: right;">
                                    <button class="btn btn-success btn-sm" type="button" style="width: 50%;padding: 8px; margin-top: 20px; right: 15px;" onclick="AddProduct(this)">
                                        <i class="fa fa-plus fa-2x"></i>
                                    </button>
                                    <button class="btn btn-default btn-sm" type="button" style="width: 40%;padding: 8px; margin-top: 20px; right: 15px; margin-left:10px" onclick="clearFeilds()">Clear</button>
                                </div>
                                <div class="makespace"></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <div class="col-xs-12">
                            <div class="table-responsive">
                                <table id="tblVehLocTrans" class="table table-striped table-hover responsive">
                                    <thead>
                                        <tr>
                                            <th>Sr</th>
                                            <th>Chasis No</th>
                                            <th>Engine No</th>
                                            <th>Brand</th>
                                            <th>Product</th>
                                            <th>Model</th>
                                            <th>Reg No</th>
                                            <th>Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody style="font-size:12px; text-transform: uppercase"></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="pull-right">
                                @*<button type="reset" class="btn btn-default" id="btnCancel" style="margin-top:25px;">Cancel</button>*@
                                <button type="button" class="btn btn-primary" id="btnSave" style="margin-top:25px;">Save</button>
                                @*<button type="button" class="btn btn-danger" id="btnDelete" style="margin-top:25px;" disabled>Delete</button>*@
                                <a class="btn btn-primary" id="btnPrint" style="margin-top:25px;" href="#">Print</a>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </form>

    <!-- Chassis / Engine No Modal Start -->
    <div class="modal" tabindex="-1" id="ChasisModal" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="margin-top:60px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title" id="modal123Label">Chassis No / Engine No</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group row">
                        <label for="exampleInputuname" class="col-md-2">Search :</label>
                        <div class="col-md-10">
                            <input type="text" id="DOSearch" class="form-control" aria-controls="tblCE" placeholder="Search . . . ">
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="table-responsive">
                            <table id="tblCE" class="table table-striped table-hover responsive">
                                <thead>
                                    <tr>
                                        <th style="text-align:center;">Chassis No</th>
                                        <th style="text-align:center;">Engine No</th>
                                    </tr>
                                </thead>
                                <tbody style="font-size:12px; text-transform: uppercase">
                                    @*@if (ViewBag.EngineNo != null)
                                    {

                                        foreach (var item in ViewBag.EngineNo)
                                        {
                                            <tr style="text-align:center;" onclick="IssueFunc(this)">
                                                <td>
                                                    <label id="lblChasisNo">@item.ChasisNo</label>
                                                </td>

                                                <td style="text-align:center;">
                                                    <label id="lblEngineNo">@item.EngineNo</label>
                                                </td>

                                            </tr>

                                        }
                                    }*@
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    <!--Chassis / Engine No Modal End -->
    <div class="modal" id="section-confirm" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Confirm</h4>
                </div>
                <div class="modal-body">

                    <h5>Are you sure want to save ?</h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                    <button class="btn btn-success" type="button" onclick="SaveMasterEnquiry()">Save</button>
                </div>
            </div>
        </div>
    </div>
    <!-- modal end -->
    <div class="modal" id="section-confirm-Delete" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Confirm</h4>
                </div>
                <div class="modal-body">

                    <h5>Are you sure want to Delete ?</h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                    <button class="btn btn-danger" type="button" onclick="DeleteEnquiry()">Delete</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Popup box modal start -->
    <div class="modal" id="section-message" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog animated bounceInDown">
            <div class="modal-content">
                <div class="modal-header" id="modalMessage">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Success</h4>
                </div>
                <div class="modal-body">

                    <h5 id="lblMessage"></h5>

                </div>
                <div class="modal-footer">
                    <button data-dismiss="modal" class="btn btn-default" type="button">Close</button>
                </div>
            </div>
        </div>
    </div>
    <!-- modal end -->

    <!-- Vehicle Location Transfer Modal Start -->
    <div class="modal" tabindex="-1" id="VehLocTransModal" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="margin-top:60px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title" id="modal123Label">Delivery Orders</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group row">
                        <label for="exampleInputuname" class="col-md-2">Search :</label>
                        <div class="col-md-10">
                            <input type="text" id="VehLocSearch" class="form-control" aria-controls="VehLocTranTable" placeholder="Search . . . ">
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="table-responsive">
                            <table id="VehLocTranTable" class="table table-striped table-hover responsive">
                                <thead>
                                    <tr>
                                        <th>Sr</th>
                                        <th>Trans No</th>
                                        <th>Trans Date</th>
                                        <th>Remarks</th>
                                    </tr>
                                </thead>
                                <tbody style="font-size:12px; text-transform: uppercase">
                                    @{ int i = 1; }
                                    @if (ViewBag.VehLocTrans != null)
                                    {
                                        foreach (var item in ViewBag.VehLocTrans)
                                        {
                                            <tr style="text-align: left" onclick="Selected_VehLocTrans('@item.TransNo')">
                                                <td> @i</td> @{ i++; }
                                                <td class="tdDiv">
                                                    <a href="#" style="color:black">@item.TransNo</a>
                                                </td>                                                
                                                <td class="tdDiv">
                                                    <a href="#" style="color:black">@item.TransDate</a>
                                                </td>
                                                <td class="tdDiv">
                                                    <a href="#" style="color:black">@item.Remarks</a>
                                                </td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" style="text-transform: uppercase;" class="btn btn-danger waves-effect text-left" data-dismiss="modal">Close</button>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    <!-- Vehicle Location Transfer Modal End -->

</section>

<script src='@Url.Content("~/assets/js/jquery-1.11.2.min.js")' type="text/javascript"></script>

<script>

    var rowCount = 1;
    var dataCount = 1;
    var SNo = 1;

    $(document).ready(function () {

        $(".datepicker").datepicker({
            dateFormat: "dd-mm-yy"
        });
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth() + 1; //January is 0!
        var yyyy = today.getFullYear();

        if (dd < 10) {
            dd = '0' + dd
        }

        if (mm < 10) {
            mm = '0' + mm
        }

        today = dd + '-' + mm + '-' + yyyy;

        $("#txtTransDate").val(today);
    })

    function ChangeColor(value) {

        if ($(value).val() != '')
            $(value).css("border-color", "#e1e1e1");
    }

    function ShowModal(type) {

        if(type == 'Chasis')
            $('#ChasisModal').modal("show");
        else
            $('#VehLocTransModal').modal("show");

    }

    $("#ddlCurrentLoc").change(function () {

        var vehLoc = $("#ddlCurrentLoc").val();
        if (vehLoc != "0") {
            getChassisNo(vehLoc);

        }

    });

    function getChassisNo(id) {

        var rowCount = 1;
        var dataCount = 1;
        var SNo = 1;

        var leadId = id;
        var dealerCode = "@Session["DealerCode"].ToString()";
        debugger
        if (leadId != "0") {
            $.ajax({
                dataType: "json",
                async: true,
                type: 'GET',
                url: '@Url.Content("~/VehTransLoc/Select_ChassisNoForVehicleLocation")',
                data: { 'EnquiryId': leadId , 'DealerCode' : dealerCode},
                success: function (data) {

                    if (data.Success == true) {

                        $("#tblCE tbody tr").remove();
                        if (data.Response.length > 0) {

                            $.each(data.Response, function (value, data) {

                                var temp = '<tr id="DelChkListRow1' + (rowCount++) + '" data-tr-count="' + (dataCount++) + '" onclick="ProductSelect(this)">' +

                              '<td class="tdDiv" style="overflow:auto;text-align:center;"><label id="lblChasisNo">' + data.ChasisNo + '</label></td>' +
                              '<td class="tdDiv" style="overflow:auto;text-align:center;"><label id="lblEngineNo">' + data.EngineNo + '</label></td>' +
                                '</tr>';

                                $("#tblCE tbody").append(temp);

                            });

                        }
                    }
                },
                complete: function () {

                },
            });

        }

    }

    ProductSelect = function (value) {
        var leadId = $(value).closest('tr').find('#lblChasisNo').text();
        $('#txtChassisNo').css("border-color", "#e1e1e1");

        if (leadId != "0") {
            $.ajax({
                dataType: "json",
                async: true,
                type: 'GET',
                url: '@Url.Content("~/VehTransLoc/Select_VehicleDetailForVehicleTransferLocation")',
                data: { 'EnquiryId': leadId },
                success: function (data) {

                    if (data.Success == true) {
                        var item = JSON.parse(data.Response)

                        //Event Information
                        $("#txtChassisNo").val(item[0].ChasisNo);
                        $("#txtEngineNo").val(item[0].EngineNo);
                        $("#txtBrandCode").val(item[0].BrandCode);
                        $("#txtBrandDesc").val(item[0].BrandDesc);
                        $("#txtProduct").val(item[0].ProdCode);
                        $("#txtProductDesc").val(item[0].ProdDesc);
                        $("#txtVersionCode").val(item[0].VersionCode);
                        $("#txtRegNo").val(item[0].RegNo);
                    }
                },
                complete: function () {

                },
            });

        }


        $("#ChasisModal").modal("hide");
    }

    function validiadtor(control) {
        var input = control.val();
        if (input == "0" || input == "") {
            control.css("border-color", "red")
            control.focus();
            $("#section-confirm").modal("hide");
            $("#lblMessage").text("Please select the Required feild");
            $("#section-message").modal("show");
            return false;
        }
        else {
            control.css("border-color", "#ccc")
            return true;
        }

    }

    function AddProduct(value) {
        //var check = ValidateProRecDetails();
        var check = validiadtor($("#txtChassisNo"));


        if (check == true) {

            var brandCode = $("#txtBrandCode").val();
            var brandDesc = $("#txtBrandDesc").val();

            var productCode = $("#txtProduct").val();
            var product = $("#txtProductDesc").val();
            var version = $("#txtVersionCode").val();

            var chassisNo = $("#txtChassisNo").val();
            var engineNo = $("#txtEngineNo").val();
            var regNo = $("#txtRegNo").val();


            var row = $(value).parent().parent().html();
            var dt = $("#tblVehLocTrans tbody").append('<tr id="fixedRow' + (rowCount++) + '" data-tr-count="' + (dataCount++) + '">' +
                                                        '<td>' + SNo++ + '</td>' +
                                                        '<td> <label id="lblChassisNo">' + chassisNo + ' </label></td>' +
                                                        '<td> <label id="lblEngineNo">' + engineNo + ' </label></td>' +
                                                        '<td> <label id="lblBrandCode">' + brandDesc + ' </label><input type="hidden" id="hdnBrandCode" value="' + brandCode + '"/></td>' +
                                                        '<td> <label id="lblProdCode">' + product + ' </label><input type="hidden" id="hdnProdCode" value="' + productCode + '"/></td>' +
                                                        '<td> <label id="lblVersion">' + version + ' </label></td>' +
                                                        '<td> <label id="lblRegNo">' + regNo + ' </label></td>' +
                                                        '<td style="text-align:center;">' +
                                                            '<button class="btn btn-danger btn-xs" id="btnRemove" onclick="removeRow(this)"><i class="fa fa-minus fa-lg"></i></button></td>' +
                                                        '</tr>');

            clearFeilds();

        }
        else {

        }
    }

    function clearFeilds() {

        $("#txtBrandCode").val("");
        $("#txtBrandDesc").val("");
        $("#txtProduct").val("");
        $("#txtProductDesc").val("");
        $("#txtVersionCode").val("");

        $("#txtChassisNo").val("");
        $("#txtEngineNo").val("");

        $("#txtRegNo").val("");

    }

    function removeRow(value) {

        var tempRow = "fixedRow" + (parseInt($(value).parent().parent().attr("data-tr-count")) - 1);

        $("#" + tempRow).find('.remove').show();
        $(value).parent().parent().remove();

        dataCount--;
        rowCount--;
        SNo--;

    }

    $("#btnSave").click(function () {

        //var check = true;
        var check = ValidateMaster();
        if (check == true) {
            $("#section-confirm").modal("show");
        } else {

        }
    })

    function ValidateMaster() {

        var count = 0;

        var currentLoc = $("#ddlCurrentLoc").val();
        var transDate = $("#txtTransDate").val();
        var newLoc = $("#ddlNewLoc").val();

        if (currentLoc == "0" || currentLoc == null) {
            $("#ddlCurrentLoc").css("border-color", "red");
            count++;
        }

        if (transDate == "" || transDate == null) {
            $("#txtTransDate").css("border-color", "red");
            count++;
        }

        if (newLoc == "0" || newLoc == null) {
            $("#ddlNewLoc").css("border-color", "red");
            count++;
        }

        var rowCount = $('#tblVehLocTrans >tbody  >tr').length;
        if (rowCount == 0) {
            $("#tblVehLocTrans").css("border-color", "red");
        }

        if (count > 0 || rowCount == 0) {

            $("#lblMessage").html("Please fill the Required feilds");
            $("#section-message").modal("show");
            return false;
        }
        else {
            return true;
        }

    }

    function SaveMasterEnquiry() {

        var dto = {};


        dto.DealerCode = "@Session["DealerCode"].ToString()";
        dto.TransNo = $("#txtTransNo").val();
        dto.TransDate = $("#txtTransDate").val();
        dto.FromLocCode = $("#ddlCurrentLoc").val();
        dto.ToLocCode = $("#ddlNewLoc").val();
        dto.Remarks = $("#txtRemarks").val();

        $.ajax({
            dataType: "json",
            async: true,
            type: 'POST',
            url: '@Url.Content("~/VehTransLoc/Insert_VehLocTransMaster")',
            data: dto,
            success: function (data) {

                    if (data.Success == true) {
                        saveDetailEnquiry();
                    }
                    else {
                        $("#section-confirm").modal("hide")
                        $("#modalMessage").css("background-color", "#e46457");
                        $("#lblMessage").text(data.Message)
                        $("#section-message").modal('show');
                    }
            },
            complete: function () {

            },
            error: function (data) {
                $("#section-confirm").modal("hide")
                $("#modalMessage").css("background-color", "#e46457");
                $("#lblMessage").text(data.Message)
                $("#section-message").modal('show');
            }
        });
    }

    function insertRecord() {
        var obj = [];

        $("#tblVehLocTrans tbody tr").each(function () {
            var dto = {};
            dto.BrandCode = $(this).closest('tr').find('#hdnBrandCode').val();
            dto.ProdCode = $(this).closest('tr').find('#hdnProdCode').val();
            dto.VersionCode = $(this).closest('tr').find('#lblVersion').text().trim();
            dto.ChasisNo = $(this).closest('tr').find('#lblChassisNo').text().trim();
            dto.EngineNo = $(this).closest('tr').find('#lblEngineNo').text().trim();
            dto.RegNo = $(this).closest('tr').find('#lblRegNo').text().trim();
            dto.ToLocCode = $("#ddlNewLoc").val();
            obj.push(dto);
        });

        return obj;
    }

    function saveDetailEnquiry(){

        var objects = [];

        objects = JSON.stringify({ 'objects' : insertRecord()});

        $.ajax({
            contentType: 'application/json; charset=utf-8',
            dataType: "json",
            //async: true,
            type: 'POST',
            url: '@Url.Content("~/VehTransLoc/Insert_VehLocTransDetail")',
            data: objects ,
            success: function (data) {

                if (data.Success == true) {
                    $("#section-confirm").modal("hide")
                    $("#modalMessage").css("background-color", "#64c344");
                    $("#lblMessage").text(data.Message)
                    $("#section-message").modal('show');
                    setTimeout(function () {
                        $('#section-message').modal('hide')
                        location.reload();
                    }, 4000);
                }
                else {
                    $("#section-confirm").modal("hide")
                    $("#modalMessage").css("background-color", "#e46457");
                    $("#lblMessage").text(data.Message)
                    $("#section-message").modal('show');

                }
            },
            complete: function () {

            },
            error: function (data) {
                $("#section-confirm").modal("hide")
                $("#modalMessage").css("background-color", "#e46457");
                $("#lblMessage").text(data.Message)
                $("#section-message").modal('show');
            }

        });

    }

    Selected_VehLocTrans = function (TransNo) {

        getData(TransNo);

        $("#VehLocTransModal").modal("hide");
    }

    function getData(id) {


        var leadId = id;
        if (leadId != "0") {
            $.ajax({
                dataType: "json",
                async: true,
                type: 'GET',
                url: '@Url.Content("~/VehTransLoc/Select_VehLocTrans")',
                data: { 'EnquiryId': leadId },
                success: function (data) {
                    if (data.Success == true) {
                        var item = JSON.parse(data.Response)


                        $("#txtTransNo").val(leadId);
                        $("#txtTrandDate").val(item[0].TransDate);
                        $("#ddlCurrentLoc").val(item[0].FromLocCode);
                        $("#ddlToLoc").val(item[0].ToLocCode);
                        $("#txtRemarks").val(item[0].Remarks);

                        $("#tblVehLocTrans tbody tr").remove()
                        //ProductDetail
                        if (item.length > 0) {
                            $.each(item, function (value, item) {

                                $("#tblVehLocTrans tbody").append('<tr id="fixedRow' + (rowCount++) + '" data-tr-count="' + (dataCount++) + '">' +
                                                        '<td>' + SNo++ + '</td>' +
                                                        '<td> <label id="lblChassisNo">' + item.ChasisNo + ' </label></td>' +
                                                        '<td> <label id="lblEngineNo">' + item.EngineNo + ' </label></td>' +
                                                        '<td> <label id="lblBrandCode">' + item.BrandDesc + ' </label><input type="hidden" id="hdnBrandCode" value="' + item.BrandCode + '"/></td>' +
                                                        '<td> <label id="lblProdCode">' + item.ProdDesc + ' </label><input type="hidden" id="hdnProdCode" value="' + item.ProdCode + '"/></td>' +
                                                        '<td> <label id="lblVersion">' + item.VersionCode + ' </label></td>' +
                                                        '<td> <label id="lblRegNo">' + item.RegNo + ' </label></td>' +
                                                        '<td style="text-align:center;">' +
                                                            '<button class="btn btn-danger btn-xs" id="btnRemove" onclick="removeRow(this)"><i class="fa fa-minus fa-lg"></i></button></td>' +
                                                        '</tr>');

                            });

                        }
                    }
                },
                complete: function () {

                },
            });

        }

    }

    $("#btnPrint").click(function () {

        var baseurl = '@Url.Action("Export")';
        var url = baseurl + '?EnquiryId=' + $('#txtTransNo').val() + '';

        //window.location.href = url;
        window.open(url, '_blank');

    })
   
</script>

